//Median in a row-wise sorted Matrix
#include<iostream>
using namespace std;
void median(int matrix[3][3]){
    int r,c;
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            if(3%2==0)
            {
               int med=(3/2)+(3+1);
               int median=med/2;
                cout<<"\n the median of row number"<<i<<" is "<<median;
               break;
            }
            else{
                int median1=3/2;
                cout<<"\n the median of row number"<<i<<" is "<<median;
                break;
            }
            
        }
    }
    
}
int main(){
    int matrix[3][3]={{1, 3, 5},{2, 6, 9},{4, 7, 10}};
    median(matrix);
    return 0;
    
}
