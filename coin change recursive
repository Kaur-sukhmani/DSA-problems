#include<iostream>
using namespace std;
int countways(int coins[],int n,int s){
    if(s==0)
    return 1;
    if(s<0)
    return 0;
    if(n==0)
    return 0;
    return countways(coins,n,s-coins[n-1])+countways(coins,n-1,s);
}
int main(){
    int coins[]={2,5,3};
    int n=3;
    int sum=5;
    countways(coins,n,sum);
    return 0;
}
