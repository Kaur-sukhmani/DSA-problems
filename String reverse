#include<iostream>
#include <bits/stdc++.h>
using namespace std;
    void reverseString (string& s) {
        int j,temp;
        int len=s.length();
        j=len-1;
        for(int i=0;i<j;i++){
        swap(s[i],s[j]);
            j--;
            
        }
    }
    
int main(){
   string s="geeksforgeeks";
   reverseString(s);
   cout<<s;
   return 0;
}
